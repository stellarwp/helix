{
    "name": "stellarwp/helix",
    "description": "An API for approachable data access in WordPress",
    "type": "library",
    "keywords": [
        "wordpress"
    ],
    "homepage": "https://github.com/stellarwp/helix",
    "license": "MIT",
    "authors": [
        {
            "name": "The Events Calendar",
            "email": "support@theeventscalendar.com"
        }
    ],
    "require": {
        "php": "^5.6",
        "lucatume/di52": "~2.1.0"
    },
    "require-dev": {
        "automattic/vipwpcs": "^2.0",
        "codeception/codeception": "^2.5.5",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
        "lucatume/function-mocker-le": "^1.0",
        "lucatume/wp-browser": "^2.2.4",
        "the-events-calendar/tribalscents": "dev-master",
        "phpunit/phpunit": "~6.0",
        "wp-cli/checksum-command": "1.0.5",
        "wp-coding-standards/wpcs": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "StellarWP\\Helix\\": "src/Helix/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
